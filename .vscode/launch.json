{
  "version": "0.2.0",
  "configurations": [
  
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["uninstall", "--delete-data", "org.gnome.gedit"],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/ysq/code/flatpak/",
      "program": "/home/ysq/code/flatpak/_build/app/flatpak",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "build"
    }
  ]
}